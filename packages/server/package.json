  "scripts": {
    "docker:up": "docker-compose -f docker-compose.yml -f docker-compose.override.yml up --build",
    "docker:restart": "docker-compose down && yarn docker:up",
    "budget": "$(npm bin)/babel-node ./cli/main.js --budget",
    "budget:create": "$(npm bin)/babel-node ./cli/main.js --create",
    "db:seed": "node ./src/data/seed.js",
    "db:setup": "npm run db:sync && npm run db:seed",
    "db:sync": "node ./src/data/sync.js",
    "prestart": "yarn db:setup",
    "start": "node ./src/server.js",
    "start:dev": "$(npm bin)/nodemon ./src/server.js",
    "test": "mocha './src/**/*.test.js'",
    "test:watch": "npm run test -- --watch"
