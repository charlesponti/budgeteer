#!/usr/bin/env node
'use strict';

process.env.NODE_ENV = 'development'
var repl = require('repl')

require('../server')

var replServer = repl.start({
  prompt: 'Backpack >    ',
  useColors: true
})

replServer.context.User = require('../server/models/user')
replServer.context.Task = require('../server/models/task')
replServer.context.Weight = require('../server/models/weight')
replServer.context.Transaction = require('../server/models/transaction')
