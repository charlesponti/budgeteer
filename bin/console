#!/usr/bin/env node
'use strict';

process.env.NODE_ENV = 'development';

var repl = require('repl');
var cthulhu = require('../server')();

var replServer = repl.start({

  prompt: 'Backpack >',

  useColors: true,

});

replServer.context.User = require('../server/models/user');
replServer.context.Task = require('../server/models/task');
replServer.context.Category = require('../server/models/category');
