{
    "plugins": [
        "prettier",
        "node"
    ],
    "rules": {
        "prettier/prettier": "error"
    },
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}