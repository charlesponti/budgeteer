{
  "name": "backpack",
  "version": "1.0.0",
  "betterScripts": {
    "server:dev": {
      "command": "nodemon",
      "env": {
        "NODE_ENV": "development",
        "PRISMA_SECRET": "my-secret-secret",
        "PRISMA_ENDPOINT": "http://localhost:4466"
      }
    }
  },
  "dependencies": {
    "@playlyfe/gql": "^2.6.1",
    "@types/graphql": "^0.13.4",
    "@types/node": "^10.9.4",
    "better-npm-run": "0.1.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "moment": "^2.22.2",
    "prisma-binding": "^2.1.5",
    "prisma-client-lib": "^1.17.1",
    "typescript": "^3.0.3",
    "util": "^0.11.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "husky": "^0.14.3",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0"
  },
  "license": "MIT",
  "main": "server/index.js",
  "private": false,
  "repository": "http://github.com/theponti/backpack",
  "scripts": {
    "precommit": "pretty-quick --lint-staged",
    "start": "node dist/index.js",
    "build": "$(npm bin)/tsc",
    "start:dev": "better-npm-run server:dev",
    "prisma:rebuild": "prisma deploy && prisma generate"
  }
}
