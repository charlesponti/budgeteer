{
  "name": "hominem-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:mongo": "sh bin/dev.sh",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:ci": "NODE_ENV=test jest --forceExit --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "coverage": "open coverage/index.html",
    "docker:up": "docker-compose up --remove-orphans --force-recreate --build -V",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "chmod +x ./bin/dev.sh",
    "seed:finance": "NODE_ENV=development ts-node lib/seedling/seed-finance.ts",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.2",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.3.9",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/platform-express": "^7.0.0",
    "@typegoose/typegoose": "^7.1.2",
    "@types/twitter": "^1.7.0",
    "apollo-server-express": "^2.14.2",
    "dotenv": "^8.2.0",
    "googleapis": "^51.0.0",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "moment": "^2.25.3",
    "mongoose": "^5.9.15",
    "papaparse": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "twitter": "^1.7.1",
    "winston": "^3.2.1",
    "snyk": "^1.336.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^14.0.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "class-validator": "^0.12.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "snyk": true
}
