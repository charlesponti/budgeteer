    "marked": "^0.3.2",
    "mongoose": "^3.9.6",
    "react": "0.x",
    "reactify": "^0.17.1",
    "serve-favicon": "^2.1.6",
    "superagent": "^0.20.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "css-loader": ">=0.6.12",
    "gulp": "^3.8.8",
    "gulp-concat": "^2.3.4",
