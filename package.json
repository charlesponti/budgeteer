{
  "name": "backpack",
  "version": "1.0.0",
  "aliasify": {
    "aliases": {
      "client/app": "./client/app/index.js",
      "underscore": "lodash"
    }
  },
  "browserify": {
    "transform": [
      "aliasify",
      "reactify"
    ]
  },
  "dependencies": {
    "aliasify": "^1.5.1",
    "amqp": "^0.2.0",
    "backbone": "^1.1.2",
    "body-parser": "^1.9.2",
    "compression": "^1.2.0",
    "connect-mongo": "^0.4.2",
    "cookie-parser": "^1.3.3",
    "cthulhu-auth": "^1.3.10",
    "express": "^4.11.0",
    "express-logger": "0.0.3",
    "express-session": "^1.10.1",
    "flux": "^2.0.1",
    "forever": "^0.13.0",
    "jquery": "^2.1.1",
    "less": "^2.1.1",
    "lodash": "^2.4.1",
    "lusca": "^1.0.2",
    "marked": "^0.3.2",
    "method-override": "^2.3.0",
    "mongoose": "^3.9.7",
    "morgan": "^1.5.0",
    "node-jsx": "^0.12.4",
    "nodemailer": "^1.3.0",
    "pm2": "^0.12.3",
    "react": "^0.12.1",
    "socket.io": "^1.3.2",
    "superagent": "^0.20.0",
    "swig": "^1.4.2",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "browserify": "^7.1.0",
    "gulp": "^3.8.8",
    "gulp-concat": "^2.3.4",
    "gulp-csso": "^0.2.9",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.1.0",
    "gulp-jshint": "^1.8.4",
    "gulp-load-plugins": "^0.8.0",
    "gulp-nodemon": "^1.0.5",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.3.2",
    "gulp-size": "^1.2.0",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.1",
    "jasmine-core": "^2.1.3",
    "jasmine-node": "^1.14.5",
    "jshint-stylish": "^0.4.0",
    "karma": "^0.12.28",
    "karma-browserify": "^1.0.1",
    "karma-chrome-launcher": "^0.1.7",
    "karma-fixture": "^0.2.1-1",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-jasmine": "^0.3.5",
    "reactify": "^0.17.1",
    "run-sequence": "^1.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "main": "server/index.js",
  "private": false,
  "repository": "http://github.com/theponti/backpack",
  "scripts": {
    "console": "node bin/console",
    "build": "gulp build",
    "build:prod": "gulp build:prod",
    "start": "NODE_ENV=production nodemon server/index.js",
    "start:debug": "NODE_ENV=development node-debug server/index.js",
    "start:dev": "gulp start:dev",
    "test": "npm run test:server; npm run test:client-single",
    "test:client": "karma start test/client/karma.conf.js",
    "test:client-single": "karma start test/client/karma.conf.js --single-run",
    "test:server": "jasmine-node test/server --color --verbose --forceexit"
  }
}
